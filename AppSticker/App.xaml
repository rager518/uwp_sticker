<Application
    x:Class="AppSticker.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:AppSticker">
    <Application.Resources>
        <Style TargetType="Button" x:Key="BackStyle">
            <Setter Property="FontFamily" Value="Segoe MDL2 Assets"/>
            <Setter Property="Content" Value="&#xE0A6;"/>
            <Setter Property="FontSize" Value="18"/>
            <Setter Property="Width" Value="40"/>
            <Setter Property="Height" Value="50"/>
        </Style>
        <Style TargetType="Button" x:Key="MenuStyle">
            <Setter Property="FontSize" Value="20"/>
            <Setter Property="Width" Value="100"/>
            <Setter Property="Height" Value="50"/>
        </Style>
        <!--<Style TargetType="TextBlock" x:Key="TitleStyle">
            <Setter Property="FontSize" Value="20"/>
            <Setter Property="FontWeight" Value="Bold"/>
            <Setter Property="Margin" Value="20 0 0 0"/>
        </Style>
        <Style TargetType="TextBlock" x:Key="MenuIconStyle">
            <Setter Property="FontFamily" Value="Segoe MDL2 Assets"/>
            <Setter Property="FontSize" Value="20"/>
        </Style>
        <Style TargetType="TextBlock" x:Key="MenuContentStyle">
            <Setter Property="FontSize" Value="18"/>
            <Setter Property="Margin" Value="20 0 0 0"/>
        </Style>-->
    </Application.Resources>
</Application>
